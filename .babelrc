{
  "env": {
    "development": {
      "presets": [
        "es2015",
        "es2016",
        "react",
        [
          "env",
          {
            "modules": false,
            "targets": {
              "browsers": [
                "last 2 versions",
                "safari >= 7"
              ],
              "uglify": true
            },
            "useBuiltIns": true
          }
        ]
      ],
      "plugins": [],
      "compact": "auto",
      "comments": true,
      "sourceMaps": true
    },
    "production": {
      "presets": [
        "es2015",
        "es2016",
        "react",
        [
          "env",
          {
            "modules": false,
            "targets": {
              "browsers": [
                "last 2 versions",
                "safari >= 7"
              ],
              "uglify": true
            },
            "useBuiltIns": true
          }
        ]
      ],
      "plugins": [],
      "compact": "auto",
      "comments": false,
      "sourceMaps": true
    }
  }
}
