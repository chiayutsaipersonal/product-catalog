{
    "brace_style": "collapse,preserve-inline",
    "end_with_newline": true,
    "eol": "\n",
    "indent_size": 4,
    "indent_with_tabs": false,
    "indent_char": " ",
    "preserve_newlines": true,

    "css": {
        "newline_between_rules": false,
        "selector_separator_newline": true,
        "space_around_combinator": false,
        "space_around_selector_separator": false
    },
    "html": {
        "extra_liners": ["head", "body", "/html"],
        "indent_body_inner_html": true,
        "indent_head_inner_html": true,
        "indent_handlebars": true,
        "indent_inner_html": true,
        "indent_scripts": "normal",
        "max_preserve_newlines": 1,
        "unformatted": [
            "a", "abbr", "area", "audio", "b", "bdi", "bdo",
            "br", "button", "canvas", "cite", "code", "data",
            "datalist", "del", "dfn", "em", "embed", "i",
            "iframe", "img", "input", "ins", "kbd", "keygen",
            "label", "map", "markmath", "meter", "noscript",
            "object", "output", "progress", "q", "ruby", "s",
            "samp", "script", "select", "small", "span", "strong",
            "sub", "sup", "svg", "template", "textarea",
            "time", "u", "var", "video", "wbr", "text"
        ],
        "content_unformatted": ["pre"],
        "wrap_attributes": "force-aligned",
        "wrap_attributes_indent_size": 4,
        "wrap_line_length": 0
    },
    "js": {
        "break_chained_methods": false,
        "comma_first": false,
        "e4x": false,
        "indent_level": 1,
        "jslint_happy": true,
        "keep_array_indentation": false,
        "keep_function_indentation": false,
        "keep_object_indentation": false,
        "max_preserve_newlines": 2,
        "operator_position": "before-newline",
        "space_after_anon_function": false,
        "space_before_conditional": true,
        "space_before_function": true,
        "space_in_empty_paren": false,
        "space_in_paren": false,
        "unescape_strings": false
    }
}
