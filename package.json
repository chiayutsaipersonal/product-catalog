  "private": true,
  "dependencies": {
    "axios": "^0.18.0",
    "bulma": "^0.7.1",
    "dotenv-safe": "^5.0.1",
    "express": "^4.16.3",
    "nuxt": "^1.4.0",
    "storyblok-nuxt": "0.0.3",
    "vue-mq": "^0.2.1"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.3",
    "bluebird": "^3.5.1",
    "convert-excel-to-json": "^1.3.0",
    "cross-env": "^5.1.5",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-vue": "^4.5.0",
    "fs-extra": "^6.0.1",
    "node-sass": "^4.9.0",
    "nodemon": "^1.17.4",
    "prettier": "^1.12.1",
