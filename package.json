{
  "name": "product-catalog",
  "version": "0.1.0",
  "description": "product catalog website with Express backend server and management app (Bulma version)",
  "author": "Chia-Yu Tsai <chiayu.tsai.personal@gmail.com>",
  "private": true,
  "main": "./dist/server/server.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chiayutsaipersonal/product-catalog.git"
  },
  "bugs": {
    "url": "https://github.com/chiayutsaipersonal/product-catalog/issues"
  },
  "homepage": "https://github.com/chiayutsaipersonal/product-catalog#readme",
  "browser": {
    "vue": "vue/dist/vue.common.js"
  },
  "browserify": {
    "transform": [
      "babelify",
      "vueify",
      "envify"
    ]
  },
  "dependencies": {
    "axios": "^0.16.2",
    "bluebird": "^3.5.0",
    "body-parser": "^1.17.2",
    "chalk": "^2.1.0",
    "cors": "^2.8.4",
    "express": "^4.15.4",
    "express-handlebars": "^3.0.0",
    "jsonwebtoken": "^7.4.3",
    "lodash.throttle": "^4.1.1",
    "morgan": "^1.8.2",
    "multer": "^1.3.0",
    "node-cron": "^1.2.1",
    "nodemailer": "^4.0.1",
    "sequelize": "^4.6.0",
    "sqlite3": "^3.1.9",
    "vue": "^2.4.2",
    "vue-router": "^2.7.0",
    "vuex": "^2.3.1",
    "world-countries": "^1.8.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "babelify": "^7.3.0",
    "browserify": "^14.4.0",
    "browserify-hmr": "^0.3.5",
    "cross-env": "^5.0.5",
    "del": "^3.0.0",
    "dotenv": "^4.0.0",
    "envify": "^4.1.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-html": "^3.2.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "file-exists-promise": "^1.0.2",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-babel": "^7.0.0",
    "gulp-eslint": "^4.0.0",
    "gulp-if": "^2.0.2",
    "gulp-load-plugins": "^1.5.0",
    "gulp-nodemon": "^2.2.1",
    "gulp-print": "^2.0.1",
    "gulp-rename": "^1.2.2",
    "gulp-sourcemaps": "^2.6.1",
    "gulp-watch": "^4.3.11",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.0.2",
    "open": "0.0.5",
    "uglify-js": "^3.0.28",
    "vueify": "^9.4.1",
    "watchify": "^3.9.0"
  },
  "scripts": {
    "client:prep": "gulp prepServer",
    "server:dev:prep": "babel src/server -d dist/server",
    "predev": "run-s client:prep server:dev:prep",
    "client:dev:transpile:watch": "watchify -vd -p browserify-hmr -e src/client/app.js -o dist/server/client/lib/app.js",
    "client:dev:html:watch": "gulp watchTemplates",
    "server:dev:babel:watch": "babel -w src/server -d dist/server",
    "server:dev:serve": "gulp nodemon",
    "client:dev:browser": "gulp startBrowser",
    "dev": "cross-env NODE_ENV=development run-p client:dev:transpile:watch client:dev:html:watch server:dev:babel:watch server:dev:serve client:dev:browser",
    "lint": "gulp lintFullSource",
    "server:prod:prep": "gulp buildServer",
    "preprod": "run-s client:prep server:prod:prep",
    "prod": "cross-env NODE_ENV=production browserify -g envify -p [ vueify/plugins/extract-css -o dist/server/client/style/app.css ] -e src/client/app.js | uglifyjs -c warnings=false -m > dist/server/client/lib/app.js"
  }
}
